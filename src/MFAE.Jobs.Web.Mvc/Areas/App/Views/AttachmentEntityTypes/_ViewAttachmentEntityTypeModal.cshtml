
@using MFAE.Jobs.Web.Areas.App.Models.Common.Modals
@using MFAE.Jobs.Web.Areas.App.Models.AttachmentEntityTypes
@model AttachmentEntityTypeViewModel

@await Html.PartialAsync("~/Areas/App/Views/Common/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel(L("Details")))

<div class="modal-body">
    <div id="AttachmentEntityTypeInformationsTab">

        <div class="row">
            <div class="col-6 mb-3">
                <h6>@L("ArName") :</h6>
                @Model.AttachmentEntityType.ArName
            </div>
            <div class="col-6 mb-3">
                <h6>@L("EnName") :</h6>
                @Model.AttachmentEntityType.EnName
            </div>
            <div class="col-6 mb-3">
                <h6>@L("Folder") :</h6>
                @Model.AttachmentEntityType.Folder
            </div>
            <div class="col-6 mb-3">
                <h6>@L("ParentTypeId") :</h6>
                @Model.AttachmentEntityType.ParentTypeId
            </div>

        </div>

    </div>
</div>

@await Html.PartialAsync("~/Areas/App/Views/Common/Modals/_ModalFooterWithClose.cshtml")